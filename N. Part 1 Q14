# Attempt 1

def food_index(foods):
    
    if 'coffee' in foods:
        return foods.index('coffee')
    
    else:
        return -1

# Attempt 2 - Added the 'donut'... messed up reading instructions

def food_index(foods):
    
    if 'coffee' in foods:
        return foods.index('coffee')
    
    elif 'donut' in foods:
        return foods.index('donut')

    else:
        return -1
        

